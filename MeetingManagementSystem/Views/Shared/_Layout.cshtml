<!doctype html>
<html lang="en">

<head>
    <title>Dashboard | Klorofil - Free Bootstrap Dashboard Template</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <!-- VENDOR CSS -->
    <!-- GOOGLE FONTS -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700" rel="stylesheet">
    <!-- ICONS -->
    <link rel="apple-touch-icon" sizes="76x76" href="assets/img/apple-icon.png">
    <link rel="icon" type="image/png" sizes="96x96" href="~/Content/Klorofil/assets/img/favicon.png">
    @*@Styles.Render("~/Content/CommonCSS")
        @Styles.Render("~/bundles/fontAwesome")*@

    @*@Scripts.Render("~/bundles/CommonJS")*@
    @Styles.Render("~/bundles/klorofilcss")
    @Styles.Render("~/bundles/klorofiljs")

</head>

<body>
    <!-- WRAPPER -->
    <div id="wrapper">
        <!-- HEADER -->
        @{
            Html.RenderPartial("~/Views/Shared/_IndexHeader.cshtml");
        }

        <!--LEFT SIDE BAR-->
        @{
            Html.RenderPartial("~/Views/Shared/_LeftSideBar.cshtml");
        }



        <!-- MAIN -->
        <div class="main">
            <!-- MAIN CONTENT -->
            <div class="main-content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>
        </div>
        <!-- END MAIN -->
        <!-- FOOTER-->
        @*<div class="footer">
            @{
                Html.RenderPartial("~/Views/Shared/_IndexFooter.cshtml");
            }
        </div>*@
        <div class="clearfix"></div>

    </div>
    <!-- END WRAPPER -->
    <!-- Javascript -->
    @RenderSection("scripts", required: false)

